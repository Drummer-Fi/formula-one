#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/mac/Documents/formula-one/node_modules/.pnpm/pnpm@6.7.4/node_modules/pnpm/bin/node_modules:/Users/mac/Documents/formula-one/node_modules/.pnpm/pnpm@6.7.4/node_modules/pnpm/node_modules:/Users/mac/Documents/formula-one/node_modules/.pnpm/pnpm@6.7.4/node_modules:/Users/mac/Documents/formula-one/node_modules/.pnpm/node_modules:/Users/mac/Documents/formula-one/node_modules:/Users/mac/Documents/node_modules:/Users/mac/node_modules:/Users/node_modules:/node_modules:/Users/mac/Documents/formula-one/node_modules/pnpm/bin/node_modules:/Users/mac/Documents/formula-one/node_modules/pnpm/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../pnpm/bin/pnpm.cjs" "$@"
else
  exec node  "$basedir/../pnpm/bin/pnpm.cjs" "$@"
fi
